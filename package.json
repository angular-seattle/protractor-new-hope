{
  "name": "protractor-new-hope",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "export UPDATE_SCREENSHOTS=0 && npm run webdriver-update && protractor protractor.conf.js",
    "pretake_goldens": "mv src/assets/frame5.png src/assets/frame4.png",
    "take_goldens": "export UPDATE_SCREENSHOTS=1 && npm run webdriver-update && protractor protractor.conf.js --specs='e2e/tractor.e2e-spec.ts' --directConnect",
    "posttake_goldens": "mv src/assets/frame4.png src/assets/frame5.png && cp src/assets/frame1.png src/assets/frame4.png",
    "webdriver-update": "webdriver-manager update --versions.chrome=2.36 --gecko false",
    "example1": "export UPDATE_SCREENSHOTS=0 && protractor protractor-tractor.conf.js",
    "example2": "node --inspect-brk ./node_modules/.bin/protractor protractor-prisoner.conf.js",
    "example3": "protractor protractor.conf.js --specs='e2e/firing-form.e2e-spec.ts'"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "6.0.0-rc.1",
    "@angular/cdk": "^5.2.4",
    "@angular/common": ">=6.0.0-rc.1 <=6.0.0-rc.1",
    "@angular/compiler": ">=6.0.0-rc.1 <=6.0.0-rc.1",
    "@angular/core": ">=6.0.0-rc.1 <=6.0.0-rc.1",
    "@angular/forms": "6.0.0-rc.1",
    "@angular/http": "6.0.0-rc.1",
    "@angular/material": "^5.2.4",
    "@angular/platform-browser": ">=6.0.0-rc.1 <=6.0.0-rc.1",
    "@angular/platform-browser-dynamic": "6.0.0-rc.1",
    "@angular/router": "6.0.0-rc.1",
    "core-js": "^2.4.1",
    "rxjs": "^6.0.0-rc.0",
    "zone.js": "~0.8.22"
  },
  "devDependencies": {
    "@angular/cli": "6.0.0-beta.8",
    "@angular/compiler-cli": "6.0.0-rc.1",
    "@angular/language-service": "6.0.0-rc.1",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^9.6.1",
    "codelyzer": "^4.2.1",
    "jasmine-core": "^3.1.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.4.2",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^1.0.0",
    "looks-same": "^3.3.0",
    "protractor": "~5.3.0",
    "ts-node": "^5.0.1",
    "tslint": "~5.9.1",
    "typescript": "2.7.2"
  }
}
