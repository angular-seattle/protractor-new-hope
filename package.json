{
  "name": "protractor-new-hope",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "export UPDATE_SCREENSHOTS=0 && npm run webdriver-update && protractor protractor.conf.js",
    "pretake_goldens": "mv src/assets/frame5.png src/assets/frame4.png",
    "take_goldens": "export UPDATE_SCREENSHOTS=1 && npm run webdriver-update && protractor protractor.conf.js --specs='e2e/tractor.e2e-spec.ts' --directConnect",
    "posttake_goldens": "mv src/assets/frame4.png src/assets/frame5.png && cp src/assets/frame1.png src/assets/frame4.png",
    "webdriver-start": "npm run webdriver-update && webdriver-manager start --versions.chrome=2.36",
    "webdriver-update": "webdriver-manager update --versions.chrome=2.36 --gecko false",
    "milestone1": "export UPDATE_SCREENSHOTS=0 && npm run webdriver-update && protractor protractor.conf.js --specs='e2e/tractor.e2e-spec.ts' --directConnect",
    "milestone2": "npm run webdriver-update && node --inspect-brk ./node_modules/.bin/protractor protractor.conf.js --specs='e2e/prisoners.e2e-spec.ts' --directConnect",
    "milestone3": "protractor protractor.conf.js --specs='e2e/firing-form.e2e-spec.ts' --highlightDelay=1000"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.7",
    "@angular/cdk": "^5.2.4",
    "@angular/common": "^5.2.7",
    "@angular/compiler": "^5.2.7",
    "@angular/core": "^5.2.7",
    "@angular/forms": "^5.2.7",
    "@angular/http": "^5.2.7",
    "@angular/material": "^5.2.4",
    "@angular/platform-browser": "^5.2.7",
    "@angular/platform-browser-dynamic": "^5.2.7",
    "@angular/router": "^5.2.7",
    "core-js": "^2.4.1",
    "rxjs": "^5.5.6",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "^1.7.2",
    "@angular/compiler-cli": "^5.2.7",
    "@angular/language-service": "^5.2.7",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.2.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.4.2",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "looks-same": "^3.3.0",
    "protractor": "~5.3.0",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "^2.5.3"
  }
}
